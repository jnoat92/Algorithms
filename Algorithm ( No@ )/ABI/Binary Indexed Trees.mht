          <TH>idx &amp; -idx</TH>
          <TH>sum</TH>
        <TR>
        <TR>
          <TD align=3Dmiddle>1</TD>
          <TD align=3Dmiddle>13 =3D 1101</TD>
          <TD align=3Dmiddle>0</TD>
          <TD align=3Dmiddle>1 (2 ^0)</TD>
          <TD align=3Dmiddle>3</TD></TR>
        <TR>
          <TD align=3Dmiddle>2</TD>
          <TD align=3Dmiddle>12 =3D 1100</TD>
          <TD align=3Dmiddle>2</TD>
          <TD align=3Dmiddle>4 (2 ^2)</TD>
          <TD align=3Dmiddle>14</TD></TR>
        <TR>
          <TD align=3Dmiddle>3</TD>
          <TD align=3Dmiddle>8 =3D 1000</TD>
          <TD align=3Dmiddle>3</TD>
          <TD align=3Dmiddle>8 (2 ^3)</TD>
          <TD align=3Dmiddle>26</TD></TR>
        <TR>
          <TD align=3Dmiddle>4</TD>
          <TD align=3Dmiddle>0 =3D 0</TD>
          <TD align=3Dmiddle>---</TD>
          <TD align=3Dmiddle>---</TD>
          <TD align=3Dmiddle>---</TD></TR></TBODY></TABLE>
